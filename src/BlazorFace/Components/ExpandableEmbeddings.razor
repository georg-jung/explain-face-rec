<div class="accordion">
    <div class="accordion-item">
        <h2 class="accordion-header">
            <button class="accordion-button collapsed" type="button" data-bs-toggle="collapse" data-bs-target="#accordion-@this.GetHashCode()" aria-expanded="false" aria-controls="accordion-@this.GetHashCode()">
                Embeddings
            </button>
        </h2>
        <div id="accordion-@this.GetHashCode()" class="accordion-collapse collapse">
            <div class="accordion-body">
                <code>@Embeddings</code>
            </div>
        </div>
    </div>
</div>

@code {
    [Parameter]
    public string? Embeddings { get; set; }
}
